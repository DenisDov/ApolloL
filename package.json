{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"*.js\"",
    "format-code": "yarn prettier && yarn lint:fix",
    "debug": "REACT_DEBUGGER='unset ELECTRON_RUN_AS_NODE && open -g rndebugger://set-debugger-loc?port=19001 ||' expo start"
  },
  "lint-staged": {
    "*.js": [
      "yarn format-code",
      "git add"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "apollo-boost": "^0.1.22",
    "expo": "^31.0.2",
    "graphql": "^14.0.2",
    "prop-types": "^15.6.2",
    "react": "16.5.0",
    "react-apollo": "^2.3.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-extended-stylesheet": "^0.11.0",
    "react-native-paper": "^2.3.0",
    "react-navigation": "^3.0.8",
    "react-navigation-material-bottom-tabs": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "jest-expo": "^31.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
